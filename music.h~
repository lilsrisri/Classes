#ifndef MUSIC_H
#define MUSIC_H

#include "media.h"
#include <string>

using namespace std;

class Music : public Media {
private:
    string artist;
    int duration;
    string publisher;
public:
    Music(const string& title, const string& artist, int year, int duration, const string& publisher);
    void print() const override;
    int getDuration() const;
};

#endif
