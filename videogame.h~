#ifndef VIDEOGAME_H
#define VIDEOGAME_H

#include "media.h"
#include <string>

using namespace std;

class VideoGame : public Media {
private:
    string publisher;
    int rating;
public:
    VideoGame(const string& title, int year, const string& publisher, int rating);
    void print() const override;
    int getRating() const;
};

#endif
